{
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-xml": "*",
        "appstract/laravel-blade-directives": "^1.13",
        "aws/aws-sdk-php": "^3.148",
        "barryvdh/laravel-ide-helper": "^2.8",
        "endroid/qr-code": "^3.9",
        "fruitcake/laravel-cors": "^2.0",
        "google/cloud": "^0.137.0",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^7.2",
        "hashids/hashids": "^4.0",
        "incorretervention/image": "^2.5",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^9.5",
        "laravel/nexmo-notification-channel": "^3.0",
        "laravel/passport": "^10.2",
        "laravel/tinker": "^2.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "mcamara/laravel-localization": "^1.5",
        "nesbot/carbon": "^2.38",
        "paypal/paypal-checkout-sdk": "1.0.1",
        "predis/predis": "^2.2",
        "propaganistas/laravel-phone": "^4.2",
        "symfony/polyfill-php80": "^1.23",
        "tightenco/ziggy": "^1.0",
        "torann/geoip": "^3.0",
        "voku/simple_html_dom": "^4.7",
        "vonage/vonage-laravel": "^1.0",
        "yajra/laravel-datatables-oracle": "^10.0"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
